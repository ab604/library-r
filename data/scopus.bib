	doi = {10.1038/mt.2011.77},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85027929861&doi=10.1038%2fmt.2011.77&partnerID=40&md5=5bbba2ae57c719c01e0f9cb7e54b4e19},
